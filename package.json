{
  "name": "lidrisp",
  "version": "1.0.0",
  "description": "A quick and dirty port of [hascheme](https://github.com/ebenpack/hascheme) into idris.",
  "main": "index.js",
  "dependencies": {
    "browserify": "^16.5.0",
    "google-closure-compiler": "^20190819.0.0"
  },
  "devDependencies": {},
  "scripts": {
    "build": "browserify index.js -s lidrisp -o dist/out.js",
    "minify": "google-closure-compiler --js=dist/out.js --js_output_file=dist/out.min.js",
    "build-prod": "npm run build && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebenpack/lidrisp.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ebenpack/lidrisp/issues"
  },
  "homepage": "https://github.com/ebenpack/lidrisp#readme"
}
